box .intro-text4 {\r\n    font-size: 2em;\r\n    font-weight: 800;\r\n  }\r\n\r\n  .intro .contents-wrap .inner .more {\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 50%;\r\n    transform: translateX(-50%);\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n  .intro .more .material-icons {\r\n    padding-bottom: 20px;\r\n    font-size: 3em;\r\n  }\r\n\r\n  /* Introduce of Section */\r\n  .introduce {\r\n    position: relative;\r\n    display: block;\r\n    width: 100%;\r\n    background-color: #e0e0e0;\r\n  }\r\n  [class=\"item-box\"] {\r\n    width: 100%;\r\n  }\r\n  [class=\"item-header\"] {\r\n    border-bottom: 2px solid #42a5f5;\r\n    margin: 30px 0;\r\n    background-color: #80d6ff;\r\n  }\r\n  [class=\"item-header\"] h2 {\r\n    font-weight: 800;\r\n    text-transform: uppercase;\r\n  }\r\n  [class=\"item-body\"] {\r\n    font-size: 1.2em;\r\n    font-weight: 600;\r\n    margin: 30px 0;\r\n  }\r\n  [class=\"item-body\"] ul li {\r\n    position: relative;\r\n    padding: 10px 0;\r\n  }\r\n  [class=\"item-body\"] ul li img {\r\n    width: 20%;\r\n  }\r\n  [class=\"item-body\"] ul li span {\r\n    position: absolute;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    left: 23%;\r\n    display: block;\r\n    width: 77%;\r\n    height: 30%;\r\n    background-color: #0077c2;\r\n  }\r\n  [class=\"item-body\"] ul li span::before {\r\n    content: \"\";\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    height: 100%;\r\n    background-color: #80d6ff;\r\n  }\r\n  [class=\"item-body\"] ul li:nth-of-type(1) span::before {\r\n    width: 90%;\r\n  }\r\n  [class=\"item-body\"] ul li:nth-of-type(2) span::before {\r\n    width: 80%;\r\n  }\r\n  [class=\"item-body\"] ul li:nth-of-type(3) span::before {\r\n    width: 60%;\r\n  }\r\n  [class=\"item-body\"] ul li span::after {\r\n    position: absolute;\r\n    left: 10px;\r\n    top: 0;\r\n  }\r\n  [class=\"item-body\"] ul li:nth-of-type(1) span::after {\